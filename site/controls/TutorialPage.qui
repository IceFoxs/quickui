<Control name="TutorialPage">

<prototype>
	<SitePage area="Tutorial">
		<sidebar>
			<TutorialNavigator id="tutorialNavigator"/>
		</sidebar>
	</SitePage>
</prototype>

<script>
$.extend(TutorialPage.prototype, {
	ready: function() {
		TutorialPage.superProto.ready.call(this);
		$(this.tutorialNavigator).control().highlightCurrentPage();
	}
});
</script>

</Control>
