<Control name="GalleryLink">

<prototype>
    <Link/>
</prototype>

<script>
$.extend(GalleryLink.prototype, {
    content: Control.Property(function(content) {
        GalleryLink.superProto.content.call(this, content);
        this.href(href="/gallery/default.html#page=" + content + "About");
    })
});
</script>

</Control>
