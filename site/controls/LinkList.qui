<Control name="LinkList">

<content>
	<List id="list">
		<p>
			<Link id="link"/>
		</p>
	</List>
</content>    

<script>
$.extend(LinkList.prototype, {
	data: Control.Element("list").controlProperty("data"),
	ready: function() {
		Control(this.list).bind = function(value) {
			$(this).find("#link").control().content(value);
		};
	}
});
</script>

</Control>