<Control name="TestDialog">
    
<prototype>
    <Dialog>
        Dialog
        <div>
        	<ButtonBase id="buttonOk">OK</ButtonBase>
        </div>
    </Dialog>
</prototype>

<script>
TestDialog.prototype.extend({
	initialize: function() {
	    TestDialog.superclass.prototype.initialize.call(this);
		var self = this;
		this.$buttonOk().click(function() {
			self.close();
		});
	}	
});
</script>

</Control>
